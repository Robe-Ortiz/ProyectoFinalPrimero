<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane style="-fx-background-color: grey;"
	xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="es.finalPrimerCurso.finalPrimerCurso.PantallaFinalController">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints />
		<RowConstraints />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label alignment="CENTER" contentDisplay="CENTER" prefHeight="86.0"
			prefWidth="600.0" text="¡ GRACIAS POR JUGAR !"
			textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="1"
			GridPane.halignment="CENTER" GridPane.rowIndex="1">
			<font>
				<Font size="50.0" />
			</font>
		</Label>
		<Button mnemonicParsing="false" onAction="#btnDescargarPDF"
			prefHeight="26.0" prefWidth="500.0" stylesheets="@estilos.css"
			text="Descargar PDF  con las preguntas" GridPane.columnIndex="1"
			GridPane.halignment="CENTER" GridPane.rowIndex="5" />
		<Button mnemonicParsing="false" onAction="#btnDescargarWord"
			prefHeight="26.0" prefWidth="500.0" stylesheets="@estilos.css"
			text="Descargar Word con las preguntas" GridPane.columnIndex="1"
			GridPane.halignment="CENTER" GridPane.rowIndex="7" />
		<Button mnemonicParsing="false" onAction="#volverInicio"
			prefHeight="26.0" prefWidth="500.0" stylesheets="@estilos.css"
			text="Volver al menú de Inicio" GridPane.columnIndex="1"
			GridPane.halignment="CENTER" GridPane.rowIndex="9" />
		<Button mnemonicParsing="false" onAction="#salirSimple"
			prefHeight="26.0" prefWidth="500.0" stylesheets="@estilos.css"
			text="Salir" GridPane.columnIndex="1" GridPane.halignment="CENTER"
			GridPane.rowIndex="11" />
		<Button mnemonicParsing="false" onAction="#btnGrafica" prefHeight="54.0"
			prefWidth="500.0" stylesheets="@estilos.css"
			text="Ver gráfica de errores" GridPane.columnIndex="1"
			GridPane.halignment="CENTER" GridPane.rowIndex="3" />
	</children>
</GridPane>
